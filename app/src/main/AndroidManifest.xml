<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mycharge">

    <application
        android:name=".MainApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".BillAddActivity" android:theme="@style/AppCompatTheme" />
        <activity android:name=".BillPagerActivity" android:theme="@style/AppCompatTheme" />
<!--        <activity android:name=".LoginActivity" android:theme="@style/AppCompatTheme"/>-->
        <activity android:name=".RegisterActivity" android:theme="@style/AppCompatTheme"/>
    </application>

</manifest>